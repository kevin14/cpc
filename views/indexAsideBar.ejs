<script type="text/javascript">
    var classify = <%=classify%>;
    var menuPos = classify?$(".menulist li:eq("+classify+")")[0].offsetTop:"63px";
    $(".magic").css({top:menuPos});
    $(".menulist li").hover(function(){
        $(".magic").stop(true,true);
        $(".magic").animate({top:$(this)[0].offsetTop},100)
    })
    $(".menulist").mouseleave(function(){
        $(".magic").stop(true,true);
        $(".magic").animate({top:menuPos},300);
    })
</script>